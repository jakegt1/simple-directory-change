#!/usr/bin/python3
import sys
import argparse
import subprocess
from dir_search import DirSearch
parser = argparse.ArgumentParser()
parser.add_argument(
    "dir_string",
    help="directory for search",
)
args = parser.parse_args()

dir_search = DirSearch(args.dir_string)
match = dir_search.return_first_match()
if(match):
    print(match)
